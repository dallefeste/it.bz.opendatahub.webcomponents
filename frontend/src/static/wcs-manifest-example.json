    "configuration": {
        "tagName": "map-widget",
        "options": [
            {
                "key": "domain",
                "type": "select",
                "label": "Domain",
                "options": {
                    "values": [
                        "mobility",
                        "tourism"
                    ],
                    "default": "mobility"
                }
            },
            {
                "key": "station-types",
                "type": "multiselect",
                "label": "Station Types",
                "options": {
                    "values": [
                        "CreativeIndustry",
                        "EChargingStation",
                        "EChargingPlug"
                    ],
                    "default": []
                }
            }
        ]
    }
